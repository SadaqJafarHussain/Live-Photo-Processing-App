{
  "name": "@innovatrics/dot-smile-liveness",
  "private": false,
  "version": "7.1.0",
  "license": "Innovatrics proprietary license",
  "types": "common/src/types/smileTypes.d.ts",
  "description": "Digital onboarding semi-passive liveness",
  "keywords": [
    "semi-passive liveness",
    "capture",
    "identity",
    "camera",
    "video"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/innovatrics/dot-js-kit.git"
  },
  "homepage": "https://developers.innovatrics.com/digital-onboarding/technical/remote/dot-web-smile-liveness/latest/documentation/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && LIB=smile vite build && pnpm build:types",
    "build:pack": "pnpm build && cd dist && pnpm pack --pack-destination ../",
    "build:types": "tsc --project tsconfig.production.json && tsc-alias -p tsc-alias.json",
    "build:watch": "pnpm build --watch",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "test": "vitest run --passWithNoTests",
    "tsc": "tsc",
    "bump:version": "npm pkg set version=$VERSION"
  },
  "dependencies": {
    "@preact/signals": "^1.3.0",
    "comlink": "^4.4.1",
    "preact": "^10.22.1",
    "preact-custom-element": "^4.3.0",
    "styled-components": "^6.1.11"
  },
  "exports": {
    ".": {
      "types": "./common/src/types/smileTypes.d.ts",
      "import": "./index.mjs",
      "require": "./index.umd.js"
    }
  }
}